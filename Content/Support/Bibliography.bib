@article{addario-berryInvertingCuttreeTransform2016,
  title = {Inverting the Cut-Tree Transform},
  author = {{Addario-Berry}, Louigi and Dieuleveut, Daphn{\'e} and Goldschmidt, Christina},
  year = {2016},
  month = jun,
  abstract = {We consider fragmentations of an R-tree \$T\$ driven by cuts arriving according to a Poisson process on \$T \textbackslash times [0, \textbackslash infty)\$, where the first co-ordinate specifies the location of the cut and the second the time at which it occurs. The genealogy of such a fragmentation is encoded by the so-called cut-tree, which was introduced by Bertoin and Miermont for a fragmentation of the Brownian continuum random tree. The cut-tree was generalised by Dieuleveut to a fragmentation of the \$\textbackslash alpha\$-stable trees, \$\textbackslash alpha \textbackslash in (1, 2)\$, and by Broutin and Wang to the inhomogeneous continuum random trees of Aldous and Pitman. Remarkably, in all of these cases, the law of the cut-tree is the same as that of the original R-tree. In this paper, we develop a clean general framework for the study of cut-trees of R-trees. We then focus particularly on the problem of reconstruction: how to recover the original R-tree from its cut-tree. This has been studied in the setting of the Brownian CRT by Broutin and Wang, where they prove that it is possible to reconstruct the original tree in distribution. We describe an enrichment of the cut-tree transformation, which endows the cut tree with information we call a consistent collection of routings. We show this procedure is well-defined under minimal conditions on the R-trees. We then show that, for the case of the Brownian CRT and the \$\textbackslash alpha\$-stable trees with \$\textbackslash alpha \textbackslash in (1, 2)\$, the original tree and the Poisson process of cuts thereon can both be almost surely reconstructed from the enriched cut-trees. For the latter results, our methods make essential use of the self-similarity and re-rooting invariance of these trees.},
  archiveprefix = {arXiv},
  eprint = {1606.04825},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Addario-Berry et al_2016_Inverting the cut-tree transform.pdf;C\:\\Users\\Zhene\\Zotero\\storage\\PH8LQBVY\\1606.html},
  journal = {arXiv:1606.04825 [math]},
  keywords = {60C05 (Primary); 05C05; 60G18; 60G52; 60E07 (Secondary),Mathematics - Probability},
  primaryclass = {math}
}

@article{addario-berryScalingLimitMinimum2013,
  title = {The Scaling Limit of the Minimum Spanning Tree of the Complete Graph},
  author = {{Addario-Berry}, Louigi and Broutin, Nicolas and Goldschmidt, Christina and Miermont, Gr{\'e}gory},
  year = {2013},
  month = jan,
  abstract = {Consider the minimum spanning tree (MST) of the complete graph with n vertices, when edges are assigned independent random weights. Endow this tree with the graph distance renormalized by n\^\{1/3\} and with the uniform measure on its vertices. We show that the resulting space converges in distribution, as n tends to infinity, to a random measured metric space in the Gromov-Hausdorff-Prokhorov topology. We additionally show that the limit is a random binary R-tree and has Minkowski dimension 3 almost surely. In particular, its law is mutually singular with that of the Brownian continuum random tree or any rescaled version thereof. Our approach relies on a coupling between the MST problem and the Erd\textbackslash "os-R\textbackslash 'enyi random graph. We exploit the explicit description of the scaling limit of the Erd\textbackslash "os-R\textbackslash 'enyi random graph in the so-called critical window, established by the first three authors in an earlier paper, and provide a similar description of the scaling limit for a "critical minimum spanning forest" contained within the MST.},
  archiveprefix = {arXiv},
  eprint = {1301.1664},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Addario-Berry et al_2013_The scaling limit of the minimum spanning tree of the complete graph.pdf;C\:\\Users\\Zhene\\Zotero\\storage\\QYJIA4QI\\1301.html},
  journal = {arXiv:1301.1664 [math]},
  keywords = {60C05 (Primary); 60F05 (Secondary),Mathematics - Combinatorics,Mathematics - Probability},
  primaryclass = {math}
}

