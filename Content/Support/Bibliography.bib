@article{goldschmidtSpreadFireRandom2019,
  title = {The Spread of Fire on a Random Multigraph},
  author = {Goldschmidt, Christina and Krea{\v c}i{\'c}, Eleonora},
  year = {2019},
  month = mar,
  volume = {51},
  pages = {1--40},
  issn = {0001-8678, 1475-6064},
  doi = {10.1017/apr.2019.2},
  abstract = {We study a model for the destruction of a random network by fire. Suppose that we are given a multigraph of minimum degree at least 2 having real-valued edge-lengths. We pick a uniform point from along the length and set it alight; the edges of the multigraph burn at speed 1. If the fire reaches a vertex of degree 2, the fire gets directly passed on to the neighbouring edge; a vertex of degree at least 3, however, passes the fire either to all of its neighbours or none, each with probability \$1/2\$. If the fire goes out before the whole network is burnt, we again set fire to a uniform point. We are interested in the number of fires which must be set in order to burn the whole network, and the number of points which are burnt from two different directions. We analyse these quantities for a random multigraph having \$n\$ vertices of degree 3 and \$\textbackslash alpha(n)\$ vertices of degree 4, where \$\textbackslash alpha(n)/n \textbackslash to 0\$ as \$n \textbackslash to \textbackslash infty\$, with i.i.d. standard exponential edge-lengths. Depending on whether \$\textbackslash alpha(n) \textbackslash gg \textbackslash sqrt\{n\}\$ or \$\textbackslash alpha(n)=O(\textbackslash sqrt\{n\})\$, we prove that as \$n \textbackslash to \textbackslash infty\$ these quantities converge jointly in distribution when suitably rescaled to either a pair of constants or to (complicated) functionals of Brownian motion. We use our analysis of this model to make progress towards a conjecture of Aronson, Frieze and Pittel concerning the number of vertices which remain unmatched when we use the Karp-Sipser algorithm to find a matching on the Erd\textbackslash H\{o\}s-R\textbackslash 'enyi random graph.},
  archiveprefix = {arXiv},
  eprint = {1710.09898},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Goldschmidt_Kreačić_2019_The spread of fire on a random multigraph.pdf;C\:\\Users\\Zhene\\Zotero\\storage\\F63DFHDX\\1710.html},
  journal = {Advances in Applied Probability},
  keywords = {60C05 (Primary); 60F17; 05C80; 05C70; 05C85 (Secondary),Mathematics - Combinatorics,Mathematics - Probability},
  number = {01}
}

@article{goldschmidtStableGraphsDistributions2018,
