  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Goldschmidt_Stephenson_2019_The scaling limit of a critical random directed graph.pdf;C\:\\Users\\Zhene\\Zotero\\storage\\TBLNJ3JI\\1905.html},
  journal = {arXiv:1905.05397 [math]},
  keywords = {05C80; 60C05,directed erdos renyi,Mathematics - Combinatorics,Mathematics - Probability,scaling limits},
