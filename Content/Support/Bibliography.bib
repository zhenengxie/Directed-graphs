@article{addario-berryContinuumLimitCritical2012,
  title = {The Continuum Limit of Critical Random Graphs},
  author = {{Addario-Berry}, L. and Broutin, N. and Goldschmidt, C.},
  year = {2012},
  month = apr,
  volume = {152},
  pages = {367--406},
  issn = {1432-2064},
  doi = {10.1007/s00440-010-0325-4},
  abstract = {We consider the Erd\H{o}s\textendash R\'enyi random graph G(n, p) inside the critical window, that is when p~=~1/n~+~{$\lambda$}n-4/3, for some fixed \$\$\{\textbackslash lambda \textbackslash in \textbackslash mathbb\{R\}\}\$\$. We prove that the sequence of connected components of G(n, p), considered as metric spaces using the graph distance rescaled by n-1/3, converges towards a sequence of continuous compact metric spaces. The result relies on a bijection between graphs and certain marked random walks, and the theory of continuum random trees. Our result gives access to the answers to a great many questions about distances in critical random graphs. In particular, we deduce that the diameter of G(n, p) rescaled by n-1/3 converges in distribution to an absolutely continuous random variable with finite mean.},
  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Addario-Berry et al_2012_The continuum limit of critical random graphs.pdf},
  journal = {Probability Theory and Related Fields},
  language = {en},
  number = {3}
}

@article{addario-berryCriticalRandomGraphs2010a,
