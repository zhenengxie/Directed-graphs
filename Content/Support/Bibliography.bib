@article{kortchemskiLargeDeviationLocal2021,
  title = {Large Deviation {{Local Limit Theorems}} and Limits of Biconditioned {{Planar Maps}}},
  author = {Kortchemski, Igor and Marzouk, Cyril},
  year = {2021},
  month = apr,
  abstract = {We first establish scaling limits of nondecreasing integer-valued random walks conditioned by their terminal value at time \$n\$, in various regimes. To this end, we rely on new local limit estimates for the probability that such random walks lie at time \$n\$ at value \$x\_\{n\}\$, where the latter can be arbitrary, encompassing in particular large deviation regimes. We believe both to be of independent interest. Then, thanks to well-known bijections, this allows us to study random maps sampled at random according to face degree weights and which are conditioned at the same time by their number of vertices, edges and faces. We establish several limit theorems as these three quantities tend to infinity, depending on their relative behaviour as well as on the weight sequence. By a direct application, we confirm the prediction of Fusy \& Guitter that typical distances in uniform random bipartite maps with \$n\$ edges and \$n\^\{b\}\$ faces, respectively \$n\$ edges and \$n\^\{c\}\$ vertices, are of order \$n\^\{(2-b)/4\}\$, respectively \$n\^\{c/4\}\$; furthermore we show that in both cases, the scaling limit is the celebrated Brownian map.},
  archiveprefix = {arXiv},
  eprint = {2101.01682},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Kortchemski_Marzouk_2021_Large deviation Local Limit Theorems and limits of biconditioned Planar Maps.pdf;C\:\\Users\\Zhene\\Zotero\\storage\\YZCTST42\\2101.html},
