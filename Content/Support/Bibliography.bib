@article{AddarioBerry2017,
abstract = {Consider the minimum spanning tree (MST) of the complete graph with n vertices, when edges are assigned independent random weights. Endow this tree with the graph distance renormalized by n{\^{}}{\{}1/3{\}} and with the uniform measure on its vertices. We show that the resulting space converges in distribution, as n tends to infinity, to a random measured metric space in the Gromov-Hausdorff-Prokhorov topology. We additionally show that the limit is a random binary R-tree and has Minkowski dimension 3 almost surely. In particular, its law is mutually singular with that of the Brownian continuum random tree or any rescaled version thereof. Our approach relies on a coupling between the MST problem and the Erd$\backslash$"os-R$\backslash$'enyi random graph. We exploit the explicit description of the scaling limit of the Erd$\backslash$"os-R$\backslash$'enyi random graph in the so-called critical window, established by the first three authors in an earlier paper, and provide a similar description of the scaling limit for a "critical minimum spanning forest" contained within the MST.},
annote = {Technique of finding the scaling limit of the minimum spanning tree by exploring the critical window},
author = {Addario-Berry, Louigi and Broutin, Nicolas and Goldschmidt, Christina and Miermont, Gr{\'{e}}gory},
doi = {10.1214/16-AOP1132},
file = {:Users/Serte/Library/Application Support/Mendeley Desktop/Downloaded/Addario-Berry et al. - 2017 - The scaling limit of the minimum spanning tree of the complete graph.pdf:pdf},
issn = {00911798},
journal = {Annals of Probability},
keywords = {Critical random graphs,Gromov- Hausdorff-Prokhorov distance,Minimum spanning tree,R-graph,R-tree,Scaling limit},
month = {Sep},
number = {5},
pages = {3075--3144},
publisher = {Institute of Mathematical Statistics},
title = {{The scaling limit of the minimum spanning tree of the complete graph}},
volume = {45},
year = {2017}
}

