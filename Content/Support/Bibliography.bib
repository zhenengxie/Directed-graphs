booktitle = {Stochastic Analysis},
doi = {10.1017/CBO9780511662980.003},
editor = {Barlow, M. T. and Bingham, N. H.},
file = {:Users/Serte/Library/Application Support/Mendeley Desktop/Downloaded/Aldous - Unknown - Stochastic Analysis Proceedings of the Durham Symposium on Stochastic Analysis, 1990.pdf:pdf},
pages = {23--70},
