  eprint = {1610.08786},
  eprinttype = {arxiv},
  file = {C\:\\Users\\Zhene\\Documents\\Google Drive\\uni_stuff\\zotero\\Goldschmidt_Przykucki_2019_Parking on a random tree.pdf;C\:\\Users\\Zhene\\Zotero\\storage\\XDXRPLSU\\1610.html},
